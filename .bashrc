alias g='git'
alias gsh='git show'
alias gst='git status'
alias gbr='git branch'
alias gd='git diff'
alias ga='git add'
alias glog='git log'
alias gcm='git commit'
alias gcmm='git commit -m'
alias gcma='git commit --amend'
alias gpm='git pull origin master'
